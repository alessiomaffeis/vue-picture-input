{
  "name": "vue-picture-input",
  "version": "4.0.0",
  "description": "Mobile-friendly picture file input component with image preview and drag and drop.",
  "main": "dist/vue-picture-input.umd.js",
  "module": "dist/vue-picture-input.es.js",
  "files": [
    "dist",
    "PictureInput.vue",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "test": "jest",
    "build": "npm run build:es && npm run build:umd",
    "build:es": "BUILD_FORMAT=es vite build --mode production",
    "build:umd": "BUILD_FORMAT=umd vite build --mode production",
    "dev": "vite"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alessiomaffeis/vue-picture-input.git"
  },
  "keywords": [
    "vue",
    "picture",
    "image",
    "input",
    "preview",
    "form",
    "upload"
  ],
  "author": "alessio@maffe.is",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alessiomaffeis/vue-picture-input/issues"
  },
  "homepage": "https://github.com/alessiomaffeis/vue-picture-input",
  "peerDependencies": {
    "vue": "^2.7.0 || ^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@vitejs/plugin-vue2": "^2.3.1",
    "@vue/test-utils": "^1.3.6",
    "babel-jest": "^26.6.3",
    "jest": "^26.6.3",
    "jest-environment-jsdom": "^26.6.2",
    "jest-serializer-vue": "^3.1.0",
    "vite": "^5.0.0",
    "vite-plugin-css-injected-by-js": "^3.5.2",
    "vue": "^2.7.0",
    "vue-jest": "^4.0.1",
    "vue-server-renderer": "^2.7.0",
    "vue-template-compiler": "^2.7.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "testURL": "http://localhost/",
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      "^.+\\.js$": "babel-jest",
      ".*\\.(vue)$": "vue-jest"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ]
  },
  "unpkg": "dist/vue-picture-input.umd.js",
  "jsdelivr": "dist/vue-picture-input.umd.js"
}